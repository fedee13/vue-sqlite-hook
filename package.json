{
  "name": "vue-sqlite-hook",
  "version": "1.0.0-beta.2",
  "description": "Vue Hook for @capacitor-community/sqlite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/vue-sqlite-hook.git"
  },
  "keywords": [
    "Vue",
    "Hook",
    "Capacitor",
    "Database",
    "SQLite"
  ],
  "main": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run clean && npm run transpile && npm run copy-package",
    "copy-package": "cp ./package.json ./dist/package.json",
    "package": "npm pack ./dist",
    "deploy": "npm run build && npm test && np",
    "rollup": "rollup --config rollup.config.js",
    "transpile": "tsc --module ES2015 --target es2017 --outDir dist",
    "clean": "rimraf dist",
    "build-old": "node scripts/build.js",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@capacitor-community/sqlite": "^2.9.0",
    "@capacitor/core": "^2.4.6",
    "@types/jest": "^24.0.23",
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-plugin-vue": "^7.4.0",
    "jest": "^26.5.2",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "np": "^7.2.0",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "uglify-js": "^3.4.8"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.6",
    "@capacitor-community/sqlite": "^2.9.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist-transpiled",
      "dist",
      "test-app"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "author": "Qu√©au Jean Pierre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jepiqueau/vue-sqlite-hook/issues"
  },
  "homepage": "https://github.com/jepiqueau/vue-sqlite-hook#readme"
}
